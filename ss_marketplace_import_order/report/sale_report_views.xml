<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_order_product_search_marketplace" model="ir.ui.view">
        <field name="name">sale.report.search.marketplace</field>
        <field name="model">sale.report</field>
        <field name="inherit_id" ref="sale.view_order_product_search"/>
        <field name="arch" type="xml">
            <field name="user_id" position="before" >
                <filter name="marketplace" string="Sales Marketplace" domain="[('marketplace_order_id', '!=', False)]" />
            </field>
        </field>
    </record>

</odoo>
